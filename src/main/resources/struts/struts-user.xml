<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
    
<struts>
	<package name="loginAction" namespace="/" extends="abstract-struts">
		<action name="login" class="loginAction">
			<result name="success">/index.jsp</result>
			<result name="error">/login.jsp</result>
			<interceptor-ref name="mydefault"></interceptor-ref>
		</action>
		<action name="logout" class="loginAction" method="logout">
			<result name="success">/login.jsp</result>
			<interceptor-ref name="mydefault"></interceptor-ref>
		</action>
	</package>
	
	<package name="desktopAction" namespace="/" extends="abstract-struts">
		<action name="desktop" class="desktopAction">
			<result name="success">/desktop.jsp</result>
			<result name="error">/common/error.jsp</result>
			<interceptor-ref name="mydefault"></interceptor-ref>
		</action>
	</package>
	
	<package name="uploadAction" namespace="/api" extends="abstract-struts">
		<action name="upload" class="uploadAction">
			<result name="success">/upload.jsp</result>
			<result name="error">/upload.jsp</result>
			<interceptor-ref name="mydefault"></interceptor-ref>
		</action>
	</package>
	
	<package name="userAction" namespace="/user" extends="abstract-struts">
		<action name="direct" class="userAction">
			<result name="add">/user/userinfo.jsp</result>
			<result name="modify">/user/userinfo.jsp</result>
			<result name="info">/user/userinfo.jsp</result>
			<result name="error">/common/userinfo.jsp</result>
			<interceptor-ref name="mydefault"></interceptor-ref>
		</action>
		<action name="list" class="userAction" method="list">
			<result name="success">/user/userlist.jsp</result>
			<result name="error">/user/userlist.jsp</result>
			<interceptor-ref name="mydefault"></interceptor-ref>
		</action>
		<action name="add" class="userAction" method="addUser">
			<result name="success">/user/userinfo.jsp</result>
			<result name="error">/common/userinfo.jsp</result>
			<interceptor-ref name="mydefault"></interceptor-ref>
		</action>
		<action name="del" class="userAction" method="delUser">
			<result name="success">/user/userlist.jsp</result>
			<result name="error">/user/userlist.jsp</result>
			<interceptor-ref name="mydefault"></interceptor-ref>
		</action>
	</package>
	
	<package name="outterSystemAction" namespace="/system" extends="abstract-struts">
		<action name="show" class="outterSystemAction">
			<result name="success">/system/outtersystem.jsp</result>
			<result name="error">/system/outtersystem.jsp</result>
			<interceptor-ref name="mydefault"></interceptor-ref>
		</action>
		<action name="save" class="outterSystemAction" method="save">
			<result name="success">/system/outtersystem.jsp</result>
			<result name="error">/system/outtersystem.jsp</result>
			<interceptor-ref name="mydefault"></interceptor-ref>
		</action>
		<action name="del" class="outterSystemAction" method="delete">
			<result name="success">/system/outtersystem.jsp</result>
			<result name="error">/system/outtersystem.jsp</result>
			<interceptor-ref name="mydefault"></interceptor-ref>
		</action>
	</package>
	
	<package name="appFuncAction" namespace="/appfunc" extends="abstract-struts">
		<action name="show" class="appFuncAction">
			<result name="success">/appfunc/appfunc.jsp</result>
			<result name="error">/appfunc/appfunc.jsp</result>
			<interceptor-ref name="mydefault"></interceptor-ref>
		</action>
		<action name="save" class="appFuncAction" method="save">
			<result name="success">/appfunc/appfunc.jsp</result>
			<result name="error">/appfunc/appfunc.jsp</result>
			<interceptor-ref name="mydefault"></interceptor-ref>
		</action>
		<action name="del" class="appFuncAction" method="delete">
			<result name="success">/appfunc/appfunc.jsp</result>
			<result name="error">/appfunc/appfunc.jsp</result>
			<interceptor-ref name="mydefault"></interceptor-ref>
		</action>
	</package>
	
	<package name="dictAction" namespace="/dict" extends="abstract-struts">
		<action name="list" class="dictAction">
			<result name="success">/dictionary/dictionaryList.jsp</result>
			<result name="error">/dictionary/dictionaryList.jsp</result>
			<interceptor-ref name="mydefault"></interceptor-ref>
		</action>
		<action name="info" class="dictAction" method="info">
			<result name="success">/dictionary/dictionaryInfo.jsp</result>
			<result name="error">/dictionary/dictionaryInfo.jsp</result>
			<interceptor-ref name="mydefault"></interceptor-ref>
		</action>
		<action name="save" class="dictAction" method="save">
			<result name="success">/dictionary/dictionaryInfo.jsp</result>
			<result name="error">/dictionary/dictionaryInfo.jsp</result>
			<interceptor-ref name="mydefault"></interceptor-ref>
		</action>
		<action name="del" class="dictAction" method="delete">
			<result name="success">/dictionary/dictionaryInfo.jsp</result>
			<result name="error">/dictionary/dictionaryInfo.jsp</result>
			<interceptor-ref name="mydefault"></interceptor-ref>
		</action>
	</package>
	
	<package name="articleAction" namespace="/system/article" extends="abstract-struts">
		<action name="list" class="articleAction">
			<result name="success">/system/articleList.jsp</result>
			<result name="error">/system/articleList.jsp</result>
			<interceptor-ref name="mydefault"></interceptor-ref>
		</action>
		<action name="info" class="articleAction" method="info">
			<result name="success">/system/articleInfo.jsp</result>
			<result name="error">/system/articleInfo.jsp</result>
			<interceptor-ref name="mydefault"></interceptor-ref>
		</action>
		<action name="save" class="articleAction" method="save">
			<result name="success">/system/articleInfo.jsp</result>
			<result name="error">/system/articleInfo.jsp</result>
			<interceptor-ref name="mydefault"></interceptor-ref>
		</action>
	</package>
	<package name="feedbackAction" namespace="/system/feedback" extends="abstract-struts">
		<action name="list" class="feedbackAction">
			<result name="success">/system/feedbackList.jsp</result>
			<interceptor-ref name="mydefault"></interceptor-ref>
		</action>
		<action name="show" class="feedbackAction" method="show">
			<result name="success">/system/feedbackInfo.jsp</result>
			<interceptor-ref name="mydefault"></interceptor-ref>
		</action>
	</package>
	
	
	<package name="loginApi" namespace="/api" extends="abstract-struts">
		<action name="login" class="loginApi">
			<result name="success">/api/api.jsp</result>
		</action>
	</package>
	
	<package name="updateUserInfoApi" namespace="/api/userinfo" extends="abstract-struts">
		<action name="update" class="updateUserInfoApi">
			<result name="success">/api/api.jsp</result>
		</action>
	</package>
	
	<package name="appFuncApi" namespace="/api/func" extends="abstract-struts">
		<action name="show" class="appFuncApi">
			<result name="success">/api/api.jsp</result>
		</action>
	</package>
	
	<package name="modifyPasswordApi" namespace="/api/modifypassword" extends="abstract-struts">
		<action name="modify" class="modifyPasswordApi">
			<result name="success">/api/api.jsp</result>
		</action>
	</package>
	
	<package name="bindUserApi" namespace="/api/binduser" extends="abstract-struts">
		<action name="bind" class="bindUserApi">
			<result name="success">/api/api.jsp</result>
		</action>
	</package>
	
	<package name="statTextApi" namespace="/api/stat" extends="abstract-struts">
		<action name="test" class="statTextApi">
			<result name="success">/api/api.jsp</result>
		</action>
	</package>
	
	<package name="feedbackApi" namespace="/api/feedback" extends="abstract-struts">
		<action name="add" class="feedbackApi">
			<result name="success">/api/api.jsp</result>
		</action>
	</package>
	
</struts>